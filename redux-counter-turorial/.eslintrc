{
    "parser": "babel-eslint",
    "extends": ["airbnb", "prettier"],
    "plugins": ["react", "jsx-a11y", "import"],
    "rules": {
        "react/jsx-filename-extension": 0,
        "react/destructuring-assignment": 0,
        "react/jsx-indent": 0
    },
    "env": {
        "browser": true,
        "node": true
    }
}
